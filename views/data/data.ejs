<script>
$(document).ready(function(){
    $(".data_collection").on("click", function(){
        var col = $(this).text();
        var cols = ["users", "posts", "images", "docs"];
        var ii = 0;
        for(ii = 0; ii < cols.length; ++ii)
            if(cols[ii] == col)
                break;
        if(ii >= col.length){
            alert("集合名不存在 !");
            return;
        }
        $.get("/get-one-page-of-collection?", function(msg){
            alert(JSON.stringify(msg));
        });
    });
});
</script>
